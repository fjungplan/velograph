chainlines.cc {
    # FIX: Use 'frontend' (matches docker-compose service name)
    reverse_proxy frontend:80

    # FIX: Use 'backend' (matches docker-compose service name)
    # 'handle_path' strips the '/api' prefix so FastAPI sees '/v1/...'
    handle_path /api/* {
        reverse_proxy backend:8000
    }
}